# BULLETTRAIN_PROMPT_CHAR="└"
# BULLETTRAIN_PROMPT_CHAR="⤷"
BULLETTRAIN_PROMPT_CHAR="↳"

BULLETTRAIN_DIR_BG=088
BULLETTRAIN_TIME_BG=250
BULLETTRAIN_TIME_FG=232

BULLETTRAIN_GIT_BG=$BULLETTRAIN_TIME_BG
